<view class="containView">
  <!-- 店铺信息 -->
  <view class="weui-panel__bd">
    <view class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
      <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
        <image class="weui-media-box__thumb" src="{{shopData.img}}" />
      </view>
      <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
        <view class="weui-media-box__title">{{shopData.name}}</view>
        <!-- <view class="weui-media-box__desc">主营：{{busin}}</view>  -->
        <view class="weui-media-box__desc">{{shopData.address}}</view>
      </view>
    </view>
  </view>
  <!-- 领配产品 -->
  <view class="weuiTitle weui-panel__hd">领配产品</view>
   <view class="contain">
    <!--左侧栏-->
    <view class="nav_left">
      <block wx:for="{{cateItems}}" wx:key="{{cateItems.cate_id}}">
        <view class="nav_left_items {{curNav == item.cate_id ? 'active' : ''}}" bindtap="switchRightTab" data-index="{{index}}" data-id="{{item.cate_id}}">{{item.cate_name}}</view>
      </block>
    </view>
    <!--右侧栏-->
    <view class="nav_right">
      <!--如果有数据，才遍历项-->
      <view class="secndClass">二级分类{{cateItems.cate_name}}</view>
      <view wx:if="{{cateItems[curIndex].ishaveChild}}">
      <view>{{cateItems.cate_name}}</view>
        <block wx:for="{{cateItems[curIndex].children}}" wx:key="{{item.id}}">
          <view class="weui-panel__bd">
            <view class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
              <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
                <image class="weui-media-box__thumb proimage" src="{{item.image}}" mode="aspectFit" bindtap="showDetail" />
              </view>
              <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
                <view class="weui-media-box__title">{{item.name}}</view>
                <view class="cont">
                  <view class="proprice">{{item.price}}</view>
                  <view class="gw_num">
                    <text class="cut cut1" bindtap="cutClick" data-index="{{index}}">-</text>
                    <text class="num">{{item.num}}</text>
                    <text class="add add1" bindtap="addClick" data-index="{{index}}">+</text>
                  </view>
                </view>
              </view>
            </view>
          </view>
        </block>
      </view>
      <!--如果无数据，则显示数据-->
      <view class="nodata_text" wx:else>该分类暂无数据</view>
    </view>
  </view>


  <view class="cart">
    <view class="data weui-media-box weui-media-box_appmsg" bindtap="showCartDetail">
      <view class="icon">
        <image class='weui-media-box__thumb' src="data:image/png;base64,"/>
        <view class="count">{{cart.count}}</view>
      </view>
      <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
        <text class="weui-media-box__title">总价：{{totalPrice}}</text>
        <text class="weui-media-box__desc">总配领值：{{totalPrice}}</text>
      </view>
    </view>
    <form bindsubmit="submit" report-submit="true">
      <!--<view formType="submit" class="submit">去结算</view>-->
      <button class="parallelColler" style="background:{{backgroundColor}}" bindtap='receiveDetail'>{{showText}}</button>
      <!-- <button class="yellow {{cart.count?'':'disabled'}}" formType="submit" disabled="{{!cart.count}}">{{showText}}</button> -->
    </form>
  </view>

